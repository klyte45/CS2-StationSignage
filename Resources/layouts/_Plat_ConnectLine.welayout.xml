<WELayout>
  <self itemName="BG">
    <transform alignment="Left" arrayAxisOrder="XYZ">
      <offsetPosition x="0" y="0" z="0" />
      <offsetRotation x="-0" y="0" z="0" />
      <scale x="0.14" y="0.14" z="1" />
      <mustDraw>1</mustDraw>
      <instanceCount>-1</instanceCount>
      <arrayInstances x="1" y="1" z="1" />
      <arraySpacing x="0" y="0" z="0" />
    </transform>
    <imageMesh atlas="StationSignage:LineBg">
      <image formulae="&amp;StationSignage.Formulas.DisplayFormulas;GetLineBackgroundShape">NEW TEXT</image>
    </imageMesh>
    <defaultStyle>
      <color>FFFFFFFF</color>
      <emissiveColor>FFFFFFFF</emissiveColor>
      <metallic>0</metallic>
      <smoothness>0</smoothness>
      <emissiveIntensity>0</emissiveIntensity>
      <emissiveExposureWeight>0</emissiveExposureWeight>
      <coatStrength>0</coatStrength>
      <colorMask1 formulae="&amp;StationSignage.Formulas.PlatformFormulas;GetPlatform/&amp;StationSignage.Formulas.PlatformFormulas;GetDestinationByIdx.line/&amp;StationSignage.Formulas.LineEntityFormulas;GetLineColor">FFFFFF</colorMask1>
      <colorMask2 formulae="&amp;StationSignage.Formulas.PlatformFormulas;GetPlatform/&amp;StationSignage.Formulas.PlatformFormulas;GetDestinationByIdx.line/&amp;StationSignage.Formulas.LineEntityFormulas;GetLineColor/&amp;BelzontWE.Builtin.WEColorsFn;GetContrastColor">FFFFFF</colorMask2>
      <colorMask3>FFFFFF</colorMask3>
    </defaultStyle>
  </self>
  <children>
    <self itemName="Text">
      <transform alignment="Left" arrayAxisOrder="XYZ">
        <offsetPosition x="0" y="0" z="0" />
        <offsetRotation x="-0" y="0" z="0" />
        <scale x="0.1" y="0.1" z="1" />
        <mustDraw>1</mustDraw>
        <instanceCount>-1</instanceCount>
        <arrayInstances x="1" y="1" z="1" />
        <arraySpacing x="0" y="0" z="0" />
      </transform>
      <textMesh fontName="HelveticaWorld_Regular" rescaleHeightOnTextOverflow="true">
        <text formulae="&amp;StationSignage.Formulas.PlatformFormulas;GetPlatform/&amp;StationSignage.Formulas.PlatformFormulas;GetDestinationByIdx.line/&amp;StationSignage.Formulas.LineEntityFormulas;GetLineAcronym">NEW TEXT</text>
        <MaxWidthMeters>0.1</MaxWidthMeters>
      </textMesh>
      <defaultStyle>
        <color formulae="&amp;StationSignage.Formulas.PlatformFormulas;GetPlatform/&amp;StationSignage.Formulas.PlatformFormulas;GetDestinationByIdx.line/&amp;StationSignage.Formulas.LineEntityFormulas;GetLineColor/&amp;BelzontWE.Builtin.WEColorsFn;GetContrastColor">FFFFFFFF</color>
        <emissiveColor>FFFFFFFF</emissiveColor>
        <metallic>0</metallic>
        <smoothness>0</smoothness>
        <emissiveIntensity>0</emissiveIntensity>
        <emissiveExposureWeight>0</emissiveExposureWeight>
        <coatStrength>0</coatStrength>
        <colorMask1>FFFFFF</colorMask1>
        <colorMask2>FFFFFF</colorMask2>
        <colorMask3>FFFFFF</colorMask3>
      </defaultStyle>
    </self>
  </children>
</WELayout>